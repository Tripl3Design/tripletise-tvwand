{
  "hosting": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors https://www.tripledesign.nl https://tripledesign.nl https://www.tvwand.nl https://tvwand.nl http://127.0.0.1:5000 https://iom-develop.nl https://iom-develop.nl/tvwand https://iom-develop.nl/tvwand/configurator/"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Permissions-Policy",
            "value": "xr-spatial-tracking=(self)"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          }
        ]
      }
    ],
    "target": "relaxury-tv-wand",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "package-lock.json",
      "package.json",
      "**/TemplateData/**",
      "**/projects/tv-wand/"
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
